{
  "name": "@midnight-ntwrk/midnight-js-http-client-proof-provider",
  "version": "3.0.0",
  "description": "Implementation of proof provider based on the Midnight ledger proof server",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": "git@github.com:midnight-ntwrk/artifacts",
  "packageManager": "yarn@4.10.3",
  "author": "IOHK",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo .rollup.cache reports coverage logs src/test/resources/compiled",
    "compact": "yarn fetch-compactc && yarn compact-simple",
    "compact-simple": "run-compactc src/test/resources/simple.compact ./src/test/resources/compiled/simple",
    "build": "node ../../scripts/check-compiled.js src/test/resources/compiled && rollup -c rollup.config.mjs",
    "test": "vitest run",
    "deploy": "yarn npm publish"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@midnight-ntwrk/midnight-js-contracts": "3.0.0",
    "@midnight-ntwrk/midnight-js-network-id": "3.0.0",
    "@midnight-ntwrk/midnight-js-types": "3.0.0",
    "@midnight-ntwrk/midnight-js-utils": "3.0.0",
    "cross-fetch": "^4.1.0",
    "fetch-retry": "^6.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@midnight-ntwrk/midnight-js-compact": "3.0.0",
    "@types/lodash": "^4.17.19"
  },
  "stableVersion": "3.0.0"
}